            <div class="table-header">
                <span>Qollar:</span>
            </div>

            <table class="document-table">
                <thead>
                    <tr>
                        <th style="width: 5%">№</th>
                        <th style="width: 25%">Ad, soyad</th>
                        <th style="width: 23%">Komanda</th>
                        <th style="width: 12%">Vaxt</th>
                        <th style="width: 12%">Hesab</th>
                        <th style="width: 23%">Vəziyyət – standart, penalti, avtoqol</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        int homeScore = 0;
                        int guestScore = 0;
                    }
                    @foreach (var item in Model.MatchGoals)
                    {
                        if ((item.ClubId == Model.MatchInfo.HomeClubId && plusGoalTypes.Contains(item.GoalTypeId)) || (item.ClubId == Model.MatchInfo.GuestClubId && item.GoalTypeId == 3))
                        {
                            homeScore++;
                        }

                        if ((item.ClubId == Model.MatchInfo.GuestClubId && plusGoalTypes.Contains(item.GoalTypeId)) || (item.ClubId == Model.MatchInfo.HomeClubId && item.GoalTypeId == 3))
                        {
                            guestScore++;
                        }


                        <tr>
                            <td>@item.PlayerNumber</td>
                            <td>@item.PlayerFirstName @item.PlayerLastName</td>
                            <td>@item.ClubName</td>
                            <td>@item.Minute @(item.MinutePlus != null && item.MinutePlus > 0 ? " + " + @item.MinutePlus : "")</td>
                            <td>
                                @homeScore : @guestScore
                            </td>
                            <td>@item.GoalType</td>
                        </tr>
                    }

                </tbody>
            </table>

            <P LANG="en-US" STYLE="margin-bottom: 0.11in">
                <BR><BR>
            </P>


            if (Model.MatchPenaltyGoals != null && Model.MatchPenaltyGoals.Count > 0)
            {
                <div class="table-header">
                    <span>Oyunsonrası penallaşma qolları:</span>
                </div>

                <table class="document-table">
                    <thead>
                        <tr>
                            <th style="width: 5%">№</th>
                            <th style="width: 25%">Ad, soyad</th>
                            <th style="width: 23%">Komanda</th>
                            <th style="width: 12%">Ardıcıllıq</th>
                            <th style="width: 12%">Hesab</th>
                            <th style="width: 23%">Vəziyyət – penalti, penalti-</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            int homePenaltyScore = 0;
                            int guestPenaltyScore = 0;
                        }
                        @foreach (var item in Model.MatchPenaltyGoals)
                        {
                            if ((item.ClubId == Model.MatchInfo.HomeClubId && plusGoalTypes.Contains(item.GoalTypeId)) || (item.ClubId == Model.MatchInfo.GuestClubId && item.GoalTypeId == 3))
                            {
                                homePenaltyScore++;
                            }

                            if ((item.ClubId == Model.MatchInfo.GuestClubId && plusGoalTypes.Contains(item.GoalTypeId)) || (item.ClubId == Model.MatchInfo.HomeClubId && item.GoalTypeId == 3))
                            {
                                guestPenaltyScore++;
                            }


                            <tr>
                                <td>@item.PlayerNumber</td>
                                <td>@item.PlayerFirstName @item.PlayerLastName</td>
                                <td>@item.ClubName</td>
                                <td>@(item.PenaltyOrder)</td>
                                <td>
                                    @homePenaltyScore : @guestPenaltyScore
                                </td>
                                <td>@item.GoalType</td>
                            </tr>
                        }

                    </tbody>
                </table>

                <P LANG="en-US" STYLE="margin-bottom: 0.11in">
                    <BR><BR>
                </P>
            }






            <div class="table-header" style="page-break-before:always;">
                <span>Əvəzetmələr:</span>
            </div>

            <table class="document-table">
                <thead>
                    <tr>
                        <th colspan="4">Ev</th>
                        <th colspan="4">Qonaq</th>
                    </tr>
                    <tr>
                        <th style="width: 4%">№</th>
                        <th style="width: 20%">Meydanı tərk edən oyunçu</th>
                        <th style="width: 20%">Meydana daxil olan oyunçu</th>
                        <th style="width: 6%">Dəq.</th>
                        <th style="width: 4%">№</th>
                        <th style="width: 20%">Meydanı tərk edən oyunçu</th>
                        <th style="width: 20%">Meydana daxil olan oyunçu</th>
                        <th style="width: 6%">Dəq.</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        int rowCountMax = 0;
                        MatchPlayerShiftDto[] homeClubShifts = new MatchPlayerShiftDto[] { };
                        MatchPlayerShiftDto[] guestClubShifts = new MatchPlayerShiftDto[] { };
                        if (Model.MatchPlayerShifts.Count > 0)
                        {
                            homeClubShifts = Model.MatchPlayerShifts.Where(x => x.ClubId == Model.MatchInfo.HomeClubId).ToArray();
                            guestClubShifts = Model.MatchPlayerShifts.Where(x => x.ClubId == Model.MatchInfo.GuestClubId).ToArray();

                            rowCountMax = Math.Max(homeClubShifts.Length, guestClubShifts.Length);
                        }
                    }

                    @for (int rowNumber = 0; rowNumber < rowCountMax; rowNumber++)
                    {
                        <tr>
                            @if (rowNumber < homeClubShifts.Length)
                            {
                                <td>@(rowNumber + 1).</td>
                                <td>@homeClubShifts[rowNumber].FirstNameOut @homeClubShifts[rowNumber].LastNameOut</td>
                                <td>@homeClubShifts[rowNumber].FirstNameIn @homeClubShifts[rowNumber].LastNameIn</td>
                                <td>@homeClubShifts[rowNumber].Minute @(homeClubShifts[rowNumber].MinutePlus != null && homeClubShifts[rowNumber].MinutePlus > 0 ? " + " + homeClubShifts[rowNumber].MinutePlus : "")</td>
                            }
                            else
                            {
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                            }

                            @if (rowNumber < guestClubShifts.Length)
                            {
                                <td>@(rowNumber + 1).</td>
                                <td>@guestClubShifts[rowNumber].FirstNameOut @guestClubShifts[rowNumber].LastNameOut</td>
                                <td>@guestClubShifts[rowNumber].FirstNameIn @guestClubShifts[rowNumber].LastNameIn</td>
                                <td>@guestClubShifts[rowNumber].Minute @(guestClubShifts[rowNumber].MinutePlus != null && guestClubShifts[rowNumber].MinutePlus > 0 ? " + " + guestClubShifts[rowNumber].MinutePlus : "")</td>
                            }
                            else
                            {
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                            }
                        </tr>
                    }

                </tbody>
            </table>


            <P LANG="en-US" STYLE="margin-bottom: 0.11in">
                <BR><BR>
            </P>

            <div class="table-header">
                <span>Sarı vərəqələr:</span>
            </div>

            <table class="document-table">
                <thead>
                    <tr>
                        <th style="width: 5%">№</th>
                        <th style="width: 25%">Ad, soyad</th>
                        <th style="width: 20%">Komanda</th>
                        <th style="width: 10%">Dəq.</th>
                        <th style="width: 40%">Səbəbi</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.MatchPenaltyCards.Where(x => x.PenaltyCardType == PenaltyCardTypeEnum.Yellow))
                    {
                        string imgCard = "";
                        switch (item.PenaltyCardType)
                        {
                            case PenaltyCardTypeEnum.Yellow:
                                imgCard = "yellow";
                                break;
                            case PenaltyCardTypeEnum.Yellow2:
                                imgCard = "yellow_red";
                                break;
                            case PenaltyCardTypeEnum.Red:
                                imgCard = "red";
                                break;

                        }

                        <tr>
                            <td>@item.PlayerNumber</td>
                            <td>@item.LastName @item.FirstName</td>
                            <td>@item.ClubName</td>
                            <td>
                                <img src="/assets/images/@(imgCard).png" />

                                @if (string.IsNullOrEmpty(item.OffGameTypeName))
                                {
                                    @item.Minute @(item.MinutePlus != null && item.MinutePlus > 0 ? " + " + item.MinutePlus : "")
                                }
                                else
                                {
                                    @item.OffGameTypeName
                                }
                            </td>
                            <td>@item.Reason</td>
                        </tr>
                    }

                </tbody>
            </table>


            <P LANG="en-US" STYLE="margin-bottom: 0.11in">
                <BR><BR>
            </P>

            <div class="table-header">
                <span>Qırmızı vərəqələr:</span>
            </div>
            <table class="document-table">
