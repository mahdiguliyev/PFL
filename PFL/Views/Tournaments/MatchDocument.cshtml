                @if (Model.MatchInfo.TournamentTypeId == 3 || Model.MatchInfo.TournamentTypeId == 6 || Model.MatchInfo.TournamentTypeId == 2 || (Model.MatchInfo.TournamentTypeId == 4 && guestClubOfficialSelection.Count(x => x.ClubTypeId == 2) > 0 && guestClubOfficialSelection.Count(x => x.ClubTypeId == 1) == 0))
